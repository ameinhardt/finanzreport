{
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "target": "ES2020",
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "lib": ["ES2024"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "paths": {},
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "noUnusedParameters": true,
    "declaration": true,
    "noEmit": true,
    "sourceMap": true,
    "isolatedModules": true,
    "skipLibCheck": true
  },
  "include": ["node_modules/vite/client.d.ts", "**/*"]
}
